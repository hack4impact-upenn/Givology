
push graphic-context
	viewbox 0 0 {{width}} {{height}}

	stroke rgb(50,200,100)

	stroke-antialias 1

	fill-opacity 0

	stroke-width 1.5

	stroke-linecap round

	image src 0,0 {{width}},{{height}} "{{mapimgname}}"

	{%for l in linksto%}
		{%if x%}
			bezier {{x}},{{y}} {{l.mx}},{{l.my}} {{l.x}},{{l.y}}
		{%endif%}
		circle {{l.x}},{{l.y}} {{l.xp}},{{l.y}}
	{%endfor%}




	
	stroke rgb(50,100,200)

	{%if x%}
		circle {{x}},{{y}} {{xp}},{{y}}
	{%endif%}


pop graphic-context
