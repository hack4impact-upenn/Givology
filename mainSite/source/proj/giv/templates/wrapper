$(document).ready(function(){

//Challenge page flip on hover

	$("#pageflip").hover(function() {
		$("#pageflip img , .msg_block").stop()
			.animate({
				width: '384px',
				height: '415px'
			}, 500);
		} , function() {
		$("#pageflip img").stop()
			.animate({
				width: '60px',
				height: '60px'
			}, 900);
		$(".msg_block").stop()
			.animate({
				width: '366px',
				height: '291px'
			}, 900);
	});


});
